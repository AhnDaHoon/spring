  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.topvel.user.repository.UserMapper">
	<select id="getUser" resultType="com.topvel.user.model.User">
